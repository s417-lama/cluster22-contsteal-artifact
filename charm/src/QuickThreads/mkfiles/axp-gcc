
CC=gcc -D__AXP__ -I.

all: qt stp testpgm

qt: md/axp.h qt.c md/axp.s
	rm -f qtmd.h
	ln -s md/axp.h qtmd.h
	$(CC) -c -o qt.o qt.c
	as -o qtmds.o md/axp.s
	ar q libckqt.a qt.o qtmds.o
	$(CC) -shared -o libckqt.so qt.o qtmds.o

stp:
	$(CC) -c stp.c
	ar q libstp.a stp.o

testpgm:
	$(CC) -c meas.c
	as -o qtmdb.o md/axp_b.s
	$(CC) -o run meas.o qtmdb.o libstp.a libckqt.a
