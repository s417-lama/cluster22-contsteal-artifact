
CC=cc -D__AXP__ -I.

all: qt stp testpgm

qt: md/t3e.h qt.c md/t3e.s
	rm -f qtmd.h
	ln -s md/t3e.h qtmd.h
	$(CC) -c -o qt.o qt.c
	cam -o qtmds.o md/t3e.s
	ar q libckqt.a qt.o qtmds.o

stp:
	$(CC) -c stp.c
	ar q libstp.a stp.o

testpgm:
	$(CC) -c meas.c
	cam -o qtmdb.o md/t3e_b.s
	$(CC) -o run meas.o qtmdb.o libstp.a libckqt.a

