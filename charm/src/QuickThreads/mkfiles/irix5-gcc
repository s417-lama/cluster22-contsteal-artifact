
CC=gcc -I. -O2

all: qt stp testpgm

qt: md/mips.h qt.c md/mips-irix5.s
	rm -f qtmd.h
	ln -s md/mips.h qtmd.h
	$(CC) -pedantic -Wall -c -o qt.o qt.c
	as -n32 -o qtmds.o md/mips-irix5.s
	ar q libckqt.a qt.o qtmds.o

stp:
	$(CC) -c stp.c
	ar q libstp.a stp.o

testpgm:
	$(CC) -c meas.c
	as -o qtmdb.o md/mips_b.s
	$(CC) -o run meas.o qtmdb.o libstp.a libckqt.a
